@model IEnumerable<TimeTableManagement.Models.DEGREE_TBL>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    <a href="@Url.Action("Create", "DEGREE_TBL")" class="glyphicon glyphicon-pencil">Create New</a>  
</p>
<div class="table-agile-info">
    <table class="table">
        <tr>
            <th>
                DEPARTMENT NAME
            </th>
            <th>
               DEGREE NAME
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.DEPARTMENT_TBL.DEPARTMENT_NAME
                </td>
                <td>
                    @item.DEGREE_NAME
                </td>
                <td>
                   <a href="@Url.Action("Edit", "DEGREE_TBL", new { id = item.DEGREE_ID })" class="glyphicon glyphicon-edit"></a> |
                  <a href="@Url.Action("Details", "DEGREE_TBL", new { id = item.DEGREE_ID })" class="glyphicon glyphicon-book"></a>   |
                  <a href="@Url.Action("Delete", "DEGREE_TBL", new { id = item.DEGREE_ID })" class="glyphicon glyphicon-trash"></a>
                      
                </td>
            </tr>
        }

    </table>
    </div>