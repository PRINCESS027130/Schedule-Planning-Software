@model IEnumerable<TimeTableManagement.Models.CLASS_TBL>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    <a href="@Url.Action("Create", "CLASS_TBL")" class="glyphicon glyphicon-pencil">Create New</a>
</p>
<div class="table-agile-info">
    <table class="table">
        <tr>
            <th>
               DEGREE NAME
            </th>
            <th>
                SESSION
            </th>
            <th>
                SECTION
            </th>
            <th>
             SHIFT
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.DEGREE_TBL.DEGREE_NAME
                </td>
                <td>
                    @item.SESSION
                </td>
                <td>
                    @item.SECTION
                </td>
                <td>
                    @item.SHIFT
                </td>
                <td>
                    <a href="@Url.Action("Edit", "CLASS_TBL", new { id = item.CLASS_ID })" class="glyphicon glyphicon-edit"></a> |
                   <a href="@Url.Action("Details", "CLASS_TBL", new { id = item.CLASS_ID })" class="glyphicon glyphicon-book"></a>  |
                  <a href="@Url.Action("Delete", "CLASS_TBL", new { id = item.CLASS_ID })" class="glyphicon glyphicon-trash"></a>  
                </td>
            </tr>
        }

    </table>
    </div>