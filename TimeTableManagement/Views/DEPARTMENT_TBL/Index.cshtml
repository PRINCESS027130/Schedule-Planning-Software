@model IEnumerable<TimeTableManagement.Models.DEPARTMENT_TBL>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    <a href="@Url.Action("Create", "DEPARTMENT_TBL")" class="glyphicon glyphicon-pencil">Create New</a>
</p>
<div class="table-agile-info">
    <table class="table">
        <tr>
            <th>
               DEPARTMENT NAME
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.DEPARTMENT_NAME
                </td>
                <td>
                   <a href="@Url.Action("Edit", "DEPARTMENT_TBL", new { id = item.DEPARTMENT_ID })" class="glyphicon glyphicon-edit"></a>  |
                   <a href="@Url.Action("Details", "DEPARTMENT_TBL", new { id = item.DEPARTMENT_ID })" class="glyphicon glyphicon-book"></a>  |
                  <a href="@Url.Action("Delete", "DEPARTMENT_TBL", new { id = item.DEPARTMENT_ID })" class="glyphicon glyphicon-trash"></a>  
                </td>
            </tr>
        }

    </table>
    </div>