@model IEnumerable<TimeTableManagement.Models.COURSE_TBL>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    <a href="@Url.Action("Create", "COURSE_TBL")" class="glyphicon glyphicon-pencil">Create New</a>
</p>
<div class="table-agile-info">
    <table class="table">
        <tr>
            <th>
                DEGREE NAME
            </th>
            <th>
               COURSE NAME
            </th>
            <th>
               CREDIT HOURS
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.DEGREE_TBL.DEGREE_NAME
                </td>
                <td>
                    @item.COURSE_NAME
                </td>
                <td>
                    @item.CREDIT_HOURS
                </td>
                <td>
                    <a href="@Url.Action("Edit", "COURSE_TBL", new { id=item.COURSE_ID })" class="glyphicon glyphicon-edit"></a> |
                    <a href="@Url.Action("Details", "COURSE_TBL", new { id=item.COURSE_ID })" class="glyphicon glyphicon-book"></a>  |
                    <a href="@Url.Action("Delete", "COURSE_TBL", new { id=item.COURSE_ID })" class="glyphicon glyphicon-trash"></a>
                </td>
            </tr>
        }

    </table>
    </div>